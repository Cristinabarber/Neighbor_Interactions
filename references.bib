	keywords = {Bayesian inference, convergence of stochastic processes, {ECM}, {EM}, Gibbs sampler, importance sampling, Metropolis algorithm, multiple imputation, random-effects model, {SIR}},
	file = {Full Text PDF:C\:\\Users\\Cristina\\Zotero\\storage\\QPWACE7B\\Gelman and Rubin - 1992 - Inference from Iterative Simulation Using Multiple.pdf:application/pdf;Snapshot:C\:\\Users\\Cristina\\Zotero\\storage\\VRDRBQFT\\1177011136.html:text/html}
